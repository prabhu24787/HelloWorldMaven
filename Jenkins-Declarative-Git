pipeline {
    agent any
    
    enviornment {
        PATH = "/usr/local/share/maven/apache-maven-3.6.3:$PATH"
    }
    
    stages{
        stage("Git checkout"){
            steps{
                git 'https://github.com/prabhu24787/HelloWorldMaven.git'
            }
        }
        stage("mvn build") {
            steps{
                sh "mvn clean package"
        }
      }
    }
}
